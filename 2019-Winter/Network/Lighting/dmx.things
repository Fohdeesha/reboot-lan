// ColorStrip Mini is 4 Channel
// OPPSK Strips are 5 Channel
// Device Starting Address is Strip # Multiplied x 10

Bridge dmx:artnet-bridge:AN2 [ address="172.16.205.22", universe=0, refreshrate=30 ] {

 dimmer STRIP1MD  [dmxid="10", turnonvalue="211", turnoffvalue="0" ]
 color  STRIP1    [dmxid="11/3", fadetime=500, dimtime=500 ]
 color  STRIP2    [dmxid="20/3", fadetime=500, dimtime=500 ]
}
